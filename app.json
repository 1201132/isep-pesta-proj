{
  "expo": {
    "name": "Coach App",
    "slug": "coach-app-slug",
    "version": "1.0.0",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsVersion": "11.4.0"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show your current position on the map.",
        "NSLocationAlwaysUsageDescription": "We need your location to show your current position on the map even when the app is in the background.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow to use your location",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "bundleIdentifier": "com.1201132.coachappslug",
      "config": {
        "googleMapsApiKey": "AIzaSyC3r_xoHo2oDrv8Hbg1obXt1JWlDMipKQU"
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ],
      "package": "com.example"
    },
    "extra": {
      "eas": {
        "projectId": "4ee36c11-7853-40c5-a086-ddcb42a516a6"
      }
    }
  }
}
